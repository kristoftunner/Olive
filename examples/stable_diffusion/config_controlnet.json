{
    "input_model": {
        "type": "PyTorchModel",
        "model_loader": "controlnet_load",
        "model_script": "user_script.py",
        "io_config": {
            "input_names": [ "sample", "timestep", "encoder_hidden_states", "controlnet_cond", "conditioning_scale"],
            "output_names": [
                "down_block_0",
                "down_block_1",
                "down_block_2",
                "down_block_3",
                "down_block_4",
                "down_block_5",
                "down_block_6",
                "down_block_7",
                "down_block_8",
                "down_block_9",
                "down_block_10",
                "down_block_11",
                "mid_block"
            ]
        },
        "dummy_inputs_func": "controlnet_conversion_inputs"
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "gpu", "execution_providers": [ "DmlExecutionProvider" ] } ]
        }
    },
    "data_configs": [
        {
            "name": "latency_data_config",
            "user_script": "user_script.py",
            "load_dataset_config": { "type": "local_dataset" },
            "dataloader_config": { "type": "controlnet_data_loader", "batch_size": 1 }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "data_config": "latency_data_config",
                    "sub_types": [ { "name": "avg" } ]
                }
            ]
        }
    },
    "passes": {
        "convert": {
            "type": "OnnxConversion",
            "target_opset": 14,
            "save_as_external_data": true,
            "all_tensors_to_one_file": true,
            "external_data_name": "weights.pb"
        },
        "optimize": {
            "type": "OrtTransformersOptimization",
            "model_type": "unet",
            "opt_level": 0,
            "float16": true,
            "use_gpu": true,
            "keep_io_types": false,
            "optimization_options": {
                "enable_gelu": true,
                "enable_layer_norm": true,
                "enable_attention": true,
                "use_multi_head_attention": true,
                "enable_skip_layer_norm": false,
                "enable_embed_layer_norm": true,
                "enable_bias_skip_layer_norm": false,
                "enable_bias_gelu": true,
                "enable_gelu_approximation": false,
                "enable_qordered_matmul": false,
                "enable_shape_inference": true,
                "enable_gemm_fast_gelu": false,
                "enable_nhwc_conv": false,
                "enable_group_norm": true,
                "enable_bias_splitgelu": false,
                "enable_packed_qkv": true,
                "enable_packed_kv": true,
                "enable_bias_add": false,
                "group_norm_channels_last": false
            },
            "force_fp32_ops": [ "RandomNormalLike" ],
            "force_fp16_inputs": { "GroupNorm": [ 0, 1, 2 ] }
        },
        "optimize_cuda": {
            "type": "OrtTransformersOptimization",
            "model_type": "unet",
            "opt_level": 0,
            "float16": true,
            "use_gpu": true,
            "keep_io_types": false
        }
    },
    "pass_flows": [ [ "convert", "optimize" ] ],
    "log_severity_level": 0,
    "evaluator": "common_evaluator",
    "evaluate_input_model": false,
    "host": "local_system",
    "target": "local_system",
    "cache_dir": "cache",
    "output_dir": "footprints/controlnet"
}
